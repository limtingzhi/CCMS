<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>SearchController</servlet-name>
        <servlet-class>ccms.controller.SearchController</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProcessAddInfo</servlet-name>
        <servlet-class>ccms.controller.AddInfoController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SearchController</servlet-name>
        <url-pattern>/SearchController</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProcessAddInfo</servlet-name>
        <url-pattern>/ProcessAddInfo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>processCase</servlet-name>
        <url-pattern>/processCase</url-pattern>
    </servlet-mapping> 
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>Index.jsp</welcome-file>
    </welcome-file-list>
</web-app>
